<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context = "http://www.springframework.org/schema/context"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

 <context:annotation-config/>


    <bean class="com.demo1.Player" name="player ">
        <property name="playerName" value="MS Dhoni"/>
        <property name="team" value="India"/>
        <property name="centuries" value="10"/>
        <property name="halfCenturies" value="110"/>
    </bean>

    <bean class="com.demo1.Player" name="player1 ">
        <property name="playerName" value="Virat Kohli"/>
        <property name="team" value="India"/>
        <property name="centuries" value="27"/>
        <property name="halfCenturies" value="110"/>
    </bean>

    <bean class="com.demo1.Cricket" name="ct1 "/>


</beans>
